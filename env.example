# Dymz AI Mobile App Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration
# Using deployed backend for development
EXPO_PUBLIC_API_BASE_URL=http://104.248.43.33:8000

# For local development, use your computer's IP address, not localhost
# Find your IP with: ipconfig (Windows) or ifconfig (Mac/Linux)
# EXPO_PUBLIC_API_BASE_URL=http://192.168.1.100:8000

# For production, use your actual backend URL
# EXPO_PUBLIC_API_BASE_URL=https://api.dymz.app

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id

# App Configuration
EXPO_PUBLIC_DEEP_LINK_SCHEME=dymz
EXPO_PUBLIC_DEBUG=true
EXPO_PUBLIC_ENABLE_MOCK_DATA=false

# Development Settings
# Set to true during development for detailed logging
EXPO_PUBLIC_LOG_LEVEL=debug 